import React from 'react';
import { Line } from 'react-chartjs-2';

export default function SensorChart({ data }) {
  const chartData = {
    labels: data.timestamps,
    datasets: [{
      label: 'Temperature',
      data: data.values,
      borderColor: 'orange'
    }]
  };
  return <Line data={chartData} />;
}